{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gabibguti.github.io",
  "dependencies": {
    "@babel/helper-function-name": "^7.24.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^9.0.0",
    "craco": "^0.0.3",
    "postcss": "^8.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^7.0.4",
    "react-query": "^3.37.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "3.4.1",
